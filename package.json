    "@types/pngjs": "^6.0.5",
    "dotenv": "^16.3.1",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "openai": "^4.75.0",
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.33.0",
    "zod": "^3.22.0",
    "allure-playwright": "^3.4.5"
  }
