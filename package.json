{
  "name": "xgen-ai-e2e-tests",
  "version": "1.0.0",
  "description": "AI-driven E2E testing framework for XGEN using Playwright",
  "scripts": {
    "verify": "node verify-setup.js",
    "test:e2e": "playwright test",
