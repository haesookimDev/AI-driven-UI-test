# AI API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Test Environment
TEST_BASE_URL=http://localhost:3000
TEST_TIMEOUT=60000
TEST_HEADLESS=false

# Test Credentials
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=1234

# AI Features Toggle
ENABLE_AI_TEST_GENERATION=true
ENABLE_SELF_HEALING=true
ENABLE_AI_VISUAL_VALIDATION=true
ENABLE_ANOMALY_DETECTION=true

# AI Model Settings
AI_MODEL=gpt-5.2
AI_MODEL_FALLBACK=gpt-5.1
AI_MAX_TOKENS=4000
AI_TEMPERATURE=0.3

# Visual Testing
VISUAL_DIFF_THRESHOLD=0.05
VISUAL_UPDATE_BASELINE=false

# Performance
PERF_BASELINE_FILE=./tests/data/performance-baseline.json
